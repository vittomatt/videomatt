{
    "name": "videomatt",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prepare": "husky",
        "format": "prettier --write 'src/**/*.ts'",
        "lint": "eslint 'src/**/*.ts'",
        "lint:fix": "eslint 'src/**/*.ts' --fix --quiet && prettier --write .",
        "build": "tsc && tsc-alias",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "test:bdd": "ts-node cucumber.ts",
        "dev:users": "nodemon --watch 'src/users/**/*' --exec \"ts-node -r tsconfig-paths/register -P tsconfig.json\" src/users/index.ts",
        "dev:videos": "nodemon --watch 'src/videos/**/*' --exec \"ts-node -r tsconfig-paths/register -P tsconfig.json\" src/videos/index.ts",
        "start:users": "node dist/users/index.js",
        "start:videos": "node dist/videos/index.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "engines": {
        "node": "20.11.0"
    },
    "dependencies": {
        "@aws-sdk/client-eventbridge": "^3.800.0",
        "@aws-sdk/client-sns": "^3.731.1",
        "@aws-sdk/client-sqs": "^3.731.1",
        "@aws-sdk/credential-provider-ini": "^3.799.0",
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.18",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-async-handler": "^1.2.0",
        "helmet": "^8.0.0",
        "mongoose": "^8.14.1",
        "neverthrow": "^8.1.1",
        "pg": "^8.13.1",
        "pg-hstore": "^2.3.4",
        "pino": "^9.6.0",
        "pino-http": "^10.4.0",
        "redis": "^4.7.0",
        "sequelize": "^6.37.5",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "tsyringe": "^4.8.0",
        "uuid": "^11.0.5",
        "uuid-validate": "^0.0.3",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@cucumber/cucumber": "^12.1.0",
        "@faker-js/faker": "^9.7.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/aws-sdk": "^2.7.4",
        "@types/chai": "^5.2.1",
        "@types/dotenv-safe": "^8.1.6",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^22.10.5",
        "@types/sequelize": "^4.28.20",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^8.20.0",
        "@typescript-eslint/parser": "^8.20.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "babel-jest": "^29.7.0",
        "chai": "^4.3.7",
        "dotenv-safe": "^9.1.0",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.3.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.4.2",
        "reflect-metadata": "^0.2.2",
        "ts-jest": "^29.3.2",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3",
        "vitest": "^3.2.4"
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
